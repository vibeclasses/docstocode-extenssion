{
    "name": "docstocode-data-manager",
    "icon": "icon.png",
    "displayName": "DocsToCode Data Manager",
    "description": "Professional project management with features, bugs, and tasks tracking. To be used with the DocsToCode MCP Server.",
    "publisher": "vibeclasses",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Visualization",
        "Extension Packs",
        "Chat",
        "Education",
        "Other"
    ],
    "main": "./dist/extension.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/vibeclasses/docstocode-extension.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vibeclasses/docstocode-extension/issues"
    },
    "homepage": "https://github.com/vibeclasses/docstocode-extension#readme",
    "contributes": {
        "commands": [
            {
                "command": "docstocode.openDashboard",
                "title": "Open DocsToCode Dashboard",
                "category": "Visualization"
            }
        ],
        "keybindings": [
            {
                "command": "docstocode.openDashboard",
                "key": "ctrl+shift+d",
                "mac": "cmd+shift+d",
                "when": "projectManagerActive"
            }
        ],
        "activationEvents": [
            "workspaceContains:.docsToCode/"
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.85.0",
        "@types/node": "^20.x",
        "typescript": "^5.3.0",
        "ajv": "^8.12.0"
    },
    "dependencies": {
        "ajv": "^8.12.0",
        "ajv-formats": "^2.1.1"
    }
}
