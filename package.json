{
    "name": "docstocode",
    "icon": "icon.png",
    "displayName": "DocsToCode Data Manager",
    "description": "Professional project management with features, bugs, and tasks tracking",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "projectManager.openDashboard",
                "title": "Open DocsToCode Dashboard",
                "category": "Project Manager"
            }
        ],
        "keybindings": [
            {
                "command": "projectManager.openDashboard",
                "key": "ctrl+shift+d",
                "mac": "cmd+shift+d",
                "when": "projectManagerActive"
            }
        ],
        "activationEvents": [
            "workspaceContains:.docsToCode/"
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.85.0",
        "@types/node": "^20.x",
        "typescript": "^5.3.0",
        "ajv": "^8.12.0"
    },
    "dependencies": {
        "ajv": "^8.12.0",
        "ajv-formats": "^2.1.1"
    }
}